{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"isolatedModules": true,
		"lib": ["ES2022", "DOM", "WebWorker"],
		"module": "ES2022",
		"moduleResolution": "bundler",
		"noImplicitOverride": true,
		"noPropertyAccessFromIndexSignature": true,
		"noUncheckedIndexedAccess": true,
		"strict": true,
		"target": "ES2022"
	},
	"include": ["src"]
}
