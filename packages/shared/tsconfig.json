{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"isolatedModules": true,
		"lib": ["ESNext", "DOM", "WebWorker"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"noImplicitOverride": true,
		"noPropertyAccessFromIndexSignature": true,
		"noUncheckedIndexedAccess": true,
		"strict": true,
		"target": "ESNext"
	},
	"include": ["src"]
}
